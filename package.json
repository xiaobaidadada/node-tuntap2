{
  "name": "@xiaobaidadada/node-tuntap2",
  "version": "1.0.0",
  "description": " create tun in linux or windows ",
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "install": "prebuild-install --verbose || npm run install_build",
    "postinstall": "node src/clean.js",
    "install_build": "node-gyp rebuild && npx tsc --build ",
    "build-dev": "node-gyp rebuild --debug"
  },
  "author": "xiaobaidadada",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaobaidadada/node-tuntap2.git"
  },
  "keywords": [
    "tunnel",
    "tun",
    "tap",
    "internet",
    "vpn",
    "wintun",
    "linux",
    "windows",
    "win"
  ],
  "files": [
    "dist/*",
    "src/*",
    "index.ts",
    "binding.gyp",
    "tsconfig.json",
    "wintun_dll/*"
  ],
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^7.1.2"
  },
  "devDependencies": {
    "mocha": "^10.7.3",
    "node-addon-api": "^7.1.1",
    "node-gyp": "^10.2.0",
    "prebuild": "^13.0.1"
  }
}
